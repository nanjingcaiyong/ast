  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve   & node server.js",
    "serve:test": "vue-cli-service serve --mode test   & node server.js",
    "dev": "vue-cli-service build --mode development",
    "test": "vue-cli-service build --mode test",
    "beta": "vue-cli-service build --mode beta",
    "build:prod": "npm run  build",
    "build:beta": "vue-cli-service build --mode beta",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "plop": "plop",
    "deploy": "git add -A; git commit -m 'automated develop {DateStamp}'; git push deployment browse --force;"
  },
  "dependencies": {
    "@antv/g2plot": "^2.3.15",
